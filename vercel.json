{
  "buildCommand": "npm run vercel-build",
  "rewrites": [
    { "source": "/api/chats", "destination": "/api/express/chats" },
    { "source": "/api/chats/(.*)", "destination": "/api/express/chats/$1" },
    { "source": "/api/messages/ai", "destination": "/api/express/messages/ai" },
    { "source": "/api/testimonials", "destination": "/api/express/testimonials" }
  ]
}
